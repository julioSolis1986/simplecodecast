<div class="nested-fields">
  <%= field_set_tag "Protocolo" do %>
    <div class="row-fluid">
      <%= f.input :protocolo, :as => :radio_buttons, :collection => ['Pollock', 'McArdle'] , :label => false, :wrapper_html => { :class => 'span12' }, :item_wrapper_class => 'inline', :checked => 'Pollock' %>
    </div>
  <% end %>

  <div class='row-fluid'>
    <div class='span4'>
      <%= field_set_tag "Medidas" do %>
        <div class="row-fluid">
          <%= f.input :peso_atual, :wrapper => :prepend, :label => false, :wrapper_html => { :class => 'span12' } do %>
            <%= content_tag :div, :class => 'row-fluid' do %>
              <%= f.label :peso_atual, 'Peso Atual', :class => "span4" %>
              <%= f.text_field :peso_atual, :class => "span8" %>
            <% end %>
          <% end %>
        </div>
        <div class="row-fluid">
          <%= f.input :altura, :wrapper => :prepend, :label => false, :wrapper_html => { :class => 'span12' } do %>
            <%= content_tag :div, :class => 'row-fluid' do %>
              <%= f.label :altura, 'Altura', :class => "span4" %>
              <%= f.text_field :altura, :class => "span8" %>
            <% end %>
          <% end %>
        </div>
      <% end %>
    </div>
    <div class='span4'>
      <%= field_set_tag "Dobras" do %>
        <div class="row-fluid">
          <%= f.input :triciptal, :wrapper => :prepend, :label => false, :wrapper_html => { :class => 'span12' } do %>
            <%= content_tag :div, :class => 'row-fluid' do %>
              <%= f.label :triciptal, :class => "span4" %>
              <% if @cliente.sexo.nome == 'Feminino' %>
              <%= f.text_field :triciptal, :class => "span8" %>
              <% else %>
              <%= f.text_field :triciptal, :class => "span8", :disabled => true %>
              <% end %>
            <% end %>
          <% end %>
        </div>
        <div class="row-fluid">
          <%= f.input :subescapular, :wrapper => :prepend, :label => false, :wrapper_html => { :class => 'span12' } do %>
            <%= content_tag :div, :class => 'row-fluid' do %>
              <%= f.label :subescapular, :class => "span4" %>
              <%= f.text_field :subescapular, :class => "span8", :disabled => true %>
            <% end %>
          <% end %>
        </div>
        <div class="row-fluid">
          <%= f.input :peitoral, :wrapper => :prepend, :label => false, :wrapper_html => { :class => 'span12' } do %>
            <%= content_tag :div, :class => 'row-fluid' do %>
              <%= f.label :peitoral, :class => "span4" %>
              <% if @cliente.sexo.nome == 'Masculino' %>
              <%= f.text_field :peitoral, :class => "span8" %>
              <% else %>
              <%= f.text_field :peitoral, :class => "span8", :disabled => true %>
              <% end %>
            <% end %>
          <% end %>
        </div>
        <div class="row-fluid">
          <%= f.input :axilar, :wrapper => :prepend, :label => false, :wrapper_html => { :class => 'span12' } do %>
            <%= content_tag :div, :class => 'row-fluid' do %>
              <%= f.label :axilar, :class => "span4" %>
              <%= f.text_field :axilar, :class => "span8", :disabled => true %>
            <% end %>
          <% end %>
        </div>
        <div class="row-fluid">
          <%= f.input :supra_iliaca, :wrapper => :prepend, :label => false, :wrapper_html => { :class => 'span12' } do %>
            <%= content_tag :div, :class => 'row-fluid' do %>
              <%= f.label :supra_iliaca, :class => "span4" %>
              <% if @cliente.sexo.nome == 'Feminino' %>
              <%= f.text_field :supra_iliaca, :class => "span8" %>
              <% else %>
              <%= f.text_field :supra_iliaca, :class => "span8", :disabled => true %>
              <% end %>
            <% end %>
          <% end %>
        </div>
        <div class="row-fluid">
          <%= f.input :abdominal, :wrapper => :prepend, :label => false, :wrapper_html => { :class => 'span12' } do %>
            <%= content_tag :div, :class => 'row-fluid' do %>
              <%= f.label :abdominal, :class => "span4" %>
              <% if @cliente.sexo.nome == 'Masculino' %>
              <%= f.text_field :abdominal, :class => "span8" %>
              <% else %>
              <%= f.text_field :abdominal, :class => "span8", :disabled => true %>
              <% end %>
            <% end %>
          <% end %>
        </div>
        <div class="row-fluid">
          <%= f.input :coxa, :wrapper => :prepend, :label => false, :wrapper_html => { :class => 'span12' } do %>
            <%= content_tag :div, :class => 'row-fluid' do %>
              <%= f.label :coxa, :class => "span4" %>
              <%= f.text_field :coxa, :class => "span8" %>
            <% end %>
          <% end %>
        </div>
        <div class="row-fluid">
          <div class="input-prepend row-fluid">
            <%= content_tag :div, :class => 'row-fluid' do %>
              <%= label_tag :soma_dobras, 'Soma', :class => "span4" %>
              <%= text_field_tag :soma_dobras, '', { :class => 'span8', :disabled => true } %>
            <% end %>
          </div>
      <% end %>
    </div>
    <div class='span4'>
      <%= field_set_tag "Resultados" do %>
        <div class='row-fluid'>
          <div class='span12'>
            <div class="row-fluid">
              <div class="input-prepend row-fluid">
                <%= content_tag :div, :class => 'row-fluid' do %>
                  <%= label_tag :gordura_ideal, 'Gordura Ideal', :class => "span4" %>
                  <%= text_field_tag :gordura_ideal, '', { :class => 'span8', :disabled => true } %>
                <% end %>
              </div>
            </div>
            <div class="row-fluid">
              <div class="input-prepend row-fluid">
                <%= content_tag :div, :class => 'row-fluid' do %>
                  <%= label_tag :gordura_atual, 'Gordura Atual', :class => "span4" %>
                  <%= text_field_tag :gordura_atual, '', { :class => 'span8', :disabled => true } %>
                <% end %>
              </div>
            </div>
            <div class="row-fluid">
              <div class="input-prepend row-fluid">
                <%= content_tag :div, :class => 'row-fluid' do %>
                  <%= label_tag :peso_gordo, 'Peso Gordo', :class => "span4" %>
                  <%= text_field_tag :peso_gordo, '', { :class => 'span8', :disabled => true } %>
                <% end %>
              </div>
            </div>
            <div class="row-fluid">
              <div class="input-prepend row-fluid">
                <%= content_tag :div, :class => 'row-fluid' do %>
                  <%= label_tag :peso_magro, 'Peso Magro', :class => "span4" %>
                  <%= text_field_tag :peso_magro, '', { :class => 'span8', :disabled => true } %>
                <% end %>
              </div>
            </div>
            <div class="row-fluid">
              <div class="input-prepend row-fluid">
                <%= content_tag :div, :class => 'row-fluid' do %>
                  <%= label_tag :peso_ideal, 'Peso Ideal', :class => "span4" %>
                  <%= text_field_tag :peso_ideal, '', { :class => 'span8', :disabled => true } %>
                <% end %>
              </div>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>