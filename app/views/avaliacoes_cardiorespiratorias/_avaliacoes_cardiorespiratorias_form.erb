<div class="nested-fields">
  <%= field_set_tag "Protocolo" do %>
    <div class="row-fluid">
      <%= f.input :protocolo, :as => :check_boxes, :collection => ['Pollock', 'McArdle'] , :label => false, :wrapper_html => { :class => 'span12' } %>
    </div>
  <% end %>

  <%= field_set_tag "Medidas" do %>
    <div class='row-fluid'>
      <%= f.input :distancia_total, :as => :string, :label => 'Distância Total Percorrida', :wrapper_html => { :class => 'span6' } %>
      <%= f.input :freq_cardiaca_maxima, :as => :string, :label => 'Freq. Cardíaca Máxima', :wrapper_html => { :class => 'span6' } %>
    </div>
  <% end %>

  <%= field_set_tag "Resultados" do %>
    <div class='row-fluid'>
      <div class='span6'>
        <%= label_tag :classificao, 'Classificação' %>
        <%= text_field_tag :classificao, '', { :class => 'span12' } %>
      </div>
      <div class='span6'>
        <%= label_tag :vo2_maximo, 'VO2 Máximo' %>
        <%= text_field_tag :vo2_maximo, '', { :class => 'span12' } %>
      </div>
    </div>
  <% end %>
</div>