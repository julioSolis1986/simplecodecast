<div class="nested-fields">
  <div class="row-fluid">
    <%= f.input :postal_code, :wrapper_html => {:class => 'span4'}, :input_html => {:alt => 'cep'} %>
    <%= f.input :street, :input_html => {:class => 'input_logradouro input-block-level'}, :wrapper_html => {:class => 'span6'}, :url => autocomplete_address_street_addresses_path, :as => :autocomplete, :id_element => '', :update_elements => {} %>
    <%= f.input :number, :wrapper_html => {:class => 'span2'} %>
  </div>

  <div class="row-fluid">
    <%= f.input :complement, :wrapper_html => {:class => 'span4'} %>
    <%= f.input :neighbourhood, :input_html => {:class => 'input_bairro input-block-level'}, :wrapper_html => {:class => 'span4'}, :url => autocomplete_address_neighbourhood_addresses_path, :as => :autocomplete, :id_element => '', :update_elements => {} %>
    <%= f.association :city, :input_html => {:class => 'input_cidade input-block-level'}, :wrapper_html => {:class => 'span4'}, :default => 3412 %>
  </div>

  <div class="row-fluid">
    <%= f.association :state, :input_html => {:class => 'input_estado input-block-level'}, :wrapper_html => {:class => 'span5'}, :default => 18 %>
    <%= f.input :country, :as => :string, :wrapper_html => {:class => 'span5'}, :input_html => {:value => 'Brasil'} %>

    <div class='span2'>
      <%= link_to_remove_association 'Excluir', f, :class => 'btn btn-danger btn-remove btn-block red' %>
    </div>
  </div>
</div>